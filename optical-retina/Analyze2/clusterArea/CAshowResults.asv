E(2)=std(mDdA(Age==7))/sqrt(sum(Age==7));
E(3)=std(mDdA(Age==12))/sqrt(sum(Age==12));
E(4)=std(mDdA(Age>30))/sqrt(sum(Age>30));


errorbar([5 7 12 35],[mean(mDdA(Age==5)) mean(mDdA(Age==7)) ...
    mean(mDdA(Age==12)) mean(mDdA(Age>30))],E,'g')


